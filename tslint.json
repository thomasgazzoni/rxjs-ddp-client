{
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "max-file-line-count": [
      true,
      700
    ],
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "constructor",
      "variables-before-functions"
    ],
    "indent": [
      true,
      "spaces"
    ],
    "curly": true,
    "eofline": true,
    "new-parens": true,
    "triple-equals": true,
    "switch-default": true,
    "no-any": false,
    "no-reference": false,
    "no-duplicate-variable": true,
    "no-duplicate-key": true,
    "no-inferrable-types": true,
    "no-eval": true,
    "no-arg": true,
    "no-empty": false,
    "no-internal-module": true,
    "no-consecutive-blank-lines": true,
    "no-trailing-whitespace": true,
    "no-bitwise": false,
    "no-shadowed-variable": true,
    "no-unused-expression": true,
    "no-invalid-this": [
      true,
      "check-function-in-method"
    ],
    "no-unused-variable": [
      false,
      {
        "ignore-pattern": "^(_.*)$"
      }
    ],
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
